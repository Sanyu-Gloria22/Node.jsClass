doctype html
head
  meta(charset='UTF-8')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  title Manager&apos;s Register
  link(rel='stylesheet' href='CSS/manager.css')
  link(rel='stylesheet' href='https://cdn.jsdelivr.net/npm/bootstrap@5.3.8/dist/css/bootstrap.min.css')
form(action='/signup' method='post')
  .mb-3
    label.form-label(for='exampleInputEmail1') Email address
    input#exampleInputEmail1.form-control(type='email' aria-describedby='emailHelp' name='emailaddress')
    #emailHelp.form-text
  .mb-3
    label.form-label(for='exampleInputPassword1') Password
    input#exampleInputPassword1.form-control(type='password' name='password')
  .mb-3.form-check
    input#exampleCheck1.form-check-input(type='checkbox' name='checkbox')
    label.form-check-label(for='exampleCheck1') Check me out
  button.btn.btn-primary Submit
script(src='https://cdn.jsdelivr.net/npm/bootstrap@5.3.7/dist/js/bootstrap.bundle.min.js')
